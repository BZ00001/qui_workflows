{
  "name": "Tag torrents added more than 21 days ago so they can be excluded from cross-seed",
  "trackerPattern": "*",
  "trackerDomains": [
    "*"
  ],
  "conditions": {
    "schemaVersion": "1",
    "tag": {
      "enabled": true,
      "tags": [
        "noCS"
      ],
      "mode": "add",
      "condition": {
        "operator": "AND",
        "conditions": [
          {
            "field": "ADDED_ON_AGE",
            "operator": "GREATER_THAN",
            "value": "1814400"
          }
        ]
      }
    }
  }
}