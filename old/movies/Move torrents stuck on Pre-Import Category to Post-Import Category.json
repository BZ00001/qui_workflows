{
  "name": "Move torrents stuck on Pre-Import Category to Post-Import Category",
  "trackerPattern": "*",
  "trackerDomains": [
    "*"
  ],
  "conditions": {
    "schemaVersion": "1",
    "category": {
      "enabled": true,
      "category": "movies",
      "includeCrossSeeds": true,
      "condition": {
        "operator": "AND",
        "conditions": [
          {
            "field": "CATEGORY",
            "operator": "EQUAL",
            "value": "radarr"
          },
          {
            "field": "COMPLETION_ON_AGE",
            "operator": "GREATER_THAN_OR_EQUAL",
            "value": "21600"
          }
        ]
      }
    }
  },
  "intervalSeconds": 3600
}