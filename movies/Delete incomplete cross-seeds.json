{
  "name": "Delete incomplete cross-seeds",
  "trackerPattern": "*",
  "trackerDomains": [
    "*"
  ],
  "conditions": {
    "schemaVersion": "1",
    "delete": {
      "enabled": true,
      "mode": "deleteWithFilesPreserveCrossSeeds",
      "condition": {
        "operator": "AND",
        "conditions": [
          {
            "field": "TAGS",
            "operator": "EQUAL",
            "value": "cross-seed"
          },
          {
            "field": "PROGRESS",
            "operator": "GREATER_THAN_OR_EQUAL",
            "value": "0.96"
          },
          {
            "field": "PROGRESS",
            "operator": "LESS_THAN",
            "value": "1"
          },
          {
            "field": "ADDED_ON_AGE",
            "operator": "GREATER_THAN_OR_EQUAL",
            "value": "1800"
          },
          {
            "operator": "OR",
            "conditions": [
              {
                "field": "STATE",
                "operator": "EQUAL",
                "value": "stopped"
              },
              {
                "field": "STATE",
                "operator": "EQUAL",
                "value": "stalled"
              }
            ]
          }
        ]
      }
    }
  }
}