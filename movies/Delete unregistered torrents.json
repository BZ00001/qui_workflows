{
  "name": "Delete unregistered torrents",
  "trackerPattern": "*",
  "trackerDomains": [
    "*"
  ],
  "conditions": {
    "schemaVersion": "1",
    "delete": {
      "enabled": true,
      "mode": "deleteWithFilesPreserveCrossSeeds",
      "condition": {
        "operator": "AND",
        "conditions": [
          {
            "field": "IS_UNREGISTERED",
            "operator": "EQUAL",
            "value": "true"
          },
          {
            "field": "STATE",
            "operator": "NOT_EQUAL",
            "value": "tracker_down"
          },
          {
            "operator": "OR",
            "conditions": [
              {
                "field": "ADDED_ON_AGE",
                "operator": "GREATER_THAN_OR_EQUAL",
                "value": "259200"
              },
              {
                "field": "TAGS",
                "operator": "EQUAL",
                "value": "cross-seed"
              },
              {
                "field": "RATIO",
                "operator": "GREATER_THAN_OR_EQUAL",
                "value": "1"
              }
            ]
          }
        ]
      }
    }
  }
}