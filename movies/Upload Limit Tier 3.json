{
  "name": "Upload Limit: Tier 3",
  "trackerPattern": "*",
  "trackerDomains": [
    "*"
  ],
  "conditions": {
    "schemaVersion": "1",
    "speedLimits": {
      "enabled": true,
      "uploadKiB": 1024,
      "condition": {
        "operator": "AND",
        "conditions": [
          {
            "field": "TAGS",
            "operator": "EQUAL",
            "value": "Tier3"
          },
          {
            "operator": "OR",
            "conditions": [
              {
                "field": "RATIO",
                "operator": "GREATER_THAN_OR_EQUAL",
                "value": "2"
              },
              {
                "field": "CATEGORY",
                "operator": "EQUAL",
                "value": "cross-seed"
              }
            ]
          }
        ]
      }
    }
  }
}